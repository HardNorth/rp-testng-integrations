    <test name="Rest Assured Test">
        <classes>
            <class name="com.epam.reportportal.example.testng.logging.RestAssuredBasicLoggingTest"/>
        </classes>
    </test>
    <test name="Selenide Test">
        <classes>
            <class name="com.epam.reportportal.example.testng.logging.SelenideLoggingTest"/>
        </classes>
    </test>
    <test name="Nested Steps Test">
        <classes>
            <class name="com.epam.reportportal.example.testng.step.NestedStepsTest"/>
        </classes>
    </test>
    <test name="Nested Steps Data Provider Test">
        <classes>
            <class name="com.epam.reportportal.example.testng.step.NestedStepsWithDataProvider"/>
        </classes>
    </test>
</suite>
